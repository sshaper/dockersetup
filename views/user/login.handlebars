<main>
    <h2>Login Page</h2>
    <p>Enter username and password.  Username is "admin" password is "password"</p>
    <div class="col-md-6">
        <div class="form-group">
            <label for="username">Username</label>
            <input type="email" class="form-control" id="username" placeholder="username" value="admin">
        </div>
        <div class="form-group">
            <label for="password">Password</label>
            <input type="password" class="form-control" id="password" placeholder="Password" value="password">
        </div>
        <div class="form-group">
            <input type="button" class="btn btn-primary" id="loginBtn" value="Login">
        </div>
    </div>
</main>